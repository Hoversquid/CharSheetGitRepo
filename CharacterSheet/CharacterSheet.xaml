<Window x:Class="CharacterSheet.CharacterScreen"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CharacterSheet"
        mc:Ignorable="d"
        Title="CharacterScreen" Height="350" Width="255">
    <Grid>
        <StackPanel Name="textPanel" Height="310" VerticalAlignment="Top" Margin="100,0,0,0"/>
        <StackPanel Name="labelPanel" HorizontalAlignment="Left" Height="320" VerticalAlignment="Top" Width="95"/>
    </Grid>
</Window>
